<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <GeneDef Name="GEW_WingBase" Abstract="True">
        <displayCategory>Wings</displayCategory>
        <biostatMet>-1</biostatMet>
        <biostatCpx>1</biostatCpx>
        <exclusionTags>
            <li>Wing</li>
            <li>Wings</li>
        </exclusionTags>
    </GeneDef>

    <!-- Genetic Wings -->
    <GeneDef ParentName="GEW_WingBase">
        <defName>GEW_Genetic_Wings</defName>
        <label>Genetic Wings</label>
        <description>Carriers of this gene develop wings that mirror their hair color, flowing and textured, emerging seamlessly from their back, blending the natural hues of their locks with the majestic form of wings</description>
        <iconPath>UI/Icons/Genes/GEW_GeneticWingsIcon</iconPath>
        <displayCategory>GEW_Wings</displayCategory>
        <customEffectDescriptions>
            <li>Gains the ability to fly once every ingame hour</li>
        </customEffectDescriptions>
        <selectionWeight>0</selectionWeight>
        <symbolPack>
            <prefixSymbols>
                <li>
                    <symbol>wing</symbol>
                </li>
                <li>
                    <symbol>gentic</symbol>
                </li>
            </prefixSymbols>
        </symbolPack>

        <renderNodeProperties>
            <li>
                <workerClass>PawnRenderNodeWorker_AttachmentBody</workerClass>
                <texPath>Things/Pawn/Humanlike/BodyAttachments/GEW_GeneticWings/GEW_GeneticWings</texPath>
                <colorType>Hair</colorType>
                <overrideMeshSize>(1, 1)</overrideMeshSize>
                <parentTagDef>Body</parentTagDef>
                <rotDrawMode>Fresh, Rotting</rotDrawMode>
                <drawSize>(1.5,1.5)</drawSize>
                <drawData>
                    <scaleOffsetByBodySize>true</scaleOffsetByBodySize>
                    <defaultData>
                        <layer>-2</layer>
                    </defaultData>
                    <dataNorth>
                        <layer>299</layer>
                        <offset>(0, 0, -0.25)</offset>
                    </dataNorth>
                    <dataSouth>
                        <offset>(0, 0, 0)</offset>
                    </dataSouth>
                    <dataEast>
                        <offset>(0, 0, 0)</offset>
                    </dataEast>
                </drawData>
            </li>
        </renderNodeProperties>

        <abilities>
            <li>GEW_Wings_Fly</li>
        </abilities>
    </GeneDef>

